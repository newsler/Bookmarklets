javascript:(()=>{var option=prompt("What would you like to open?\nOption 1: Thenewladder.com\nOption 2: Bing.com\nOption 3: Custom");if(option==="1"||option==="2"||option==="3"){var i=document.createElement("iframe");i.setAttribute("id","rusic-modal");i.setAttribute("style","position:fixed;z-index:999999;width:100vw;height:100vh;border:none;");if(option==="1")i.setAttribute("src","https://newsler.wixsite.com/thenewsladder/");else if(option==="2")i.setAttribute("src","https://bing.com");else if(option==="3"){var u=prompt("Enter a website URL:");if(u!==null){u=u.trim();if(!u.startsWith("https://"))u="https://"+u;var r=/^https?:\/\/[\w\-]+(\.[\w\-]+)+[/#?]?.*$/;if(r.test(u))i.setAttribute("src",u);else{alert("Invalid URL. Please enter a valid website URL.");return;}}else{return;}}document.body.appendChild(i);i.requestFullscreen();var iframeVisible=true;alert("Iframe enabled. Press Ctrl+C to toggle visibility.");document.addEventListener('keydown',function(e){if(e.ctrlKey&&e.key==='c'){if(iframeVisible){document.body.removeChild(i);}else{document.body.appendChild(i);i.requestFullscreen();}iframeVisible=!iframeVisible;}});}else alert("Invalid option. Please choose a valid option (1, 2, or 3).");})();
